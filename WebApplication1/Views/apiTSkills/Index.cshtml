@model IEnumerable<prjMusicBetter.Models.TSkill>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FThumbnailPath)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FSkillCategory)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FThumbnailPath)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FSkillCategory.FSkillCategoryId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FSkillId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FSkillId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FSkillId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
